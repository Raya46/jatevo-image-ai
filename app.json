{
  "expo": {
    "name": "Jatevo Image Gen",
    "slug": "jatevo-image-gen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/jatevo.png",
    "scheme": "jatevoimagegen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rayk47.jatevoimagegen",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to allow you to select images to be analyzed or edited by the AI.",
        "NSCameraUsageDescription": "This app needs access to your camera to allow you to take a new photo for analysis or editing by the AI.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save images generated by the AI to your photo library."
      }
    },
    "android": {
      "package": "com.rayk47.jatevoimagegen",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/jatevo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/jatevo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/white.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "91b90dd6-5edf-4c32-9d5b-2fa6673962f6"
      }
    },
    "owner": "rayk47"
  }
}